
@{
    ViewData["Title"] = "Index";
}

@model UserViewModel
<div class="row">
    <div class="col-md-9">
        <div class="card">
                <div class="card-body">
                    <h5 class="card-title">User Info</h5>
                    <table class="table table-striped">
                    <tr>
                        <td>Username </td>
                        <td>@Model.Username </td>
                     </tr>
                     <tr>
                         <td>Email</td>
                         <td>@Model.Email</td>
                     </tr>
                    <tr>
                        <td>City</td>
                        <td>@Model.City</td>
                    </tr>

                    </table>
                </div>
        </div>

    </div>
</div>